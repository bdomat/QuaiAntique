{% extends 'base.html.twig' %}

{% block title %}Restaurant Quai Antique | Réserver
{% endblock %}

{% block body %}
	<h1 class="text-center">Réservation</h1>
	{{ form_start(form) }}
	{{ form_row(form.reservation_name) }}
	{{ form_row(form.reservation_phone) }}
	{{ form_row(form.guests_number) }}
	{{ form_row(form.date_time) }}
	<button type="submit">Réserver</button>
	{{ form_end(form) }}

{% endblock %}
